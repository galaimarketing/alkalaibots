{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules",
    "cors": [
      {
        "origin": [
          "*"
        ],
        "method": [
          "GET",
          "POST",
          "PUT",
          "DELETE",
          "OPTIONS"
        ],
        "maxAgeSeconds": 3600,
        "responseHeader": [
          "Content-Type",
          "Authorization"
        ]
      }
    ]
  }
}
